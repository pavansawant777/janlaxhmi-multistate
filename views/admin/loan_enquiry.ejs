<%- include("navbar.ejs") %>






    <div id="content" class="container-fluid bg-white p-4">
        <div class="row">
            <div class="container mt-5 mb-3">
                <h2>Export and Print Page</h2>
                <button class="btn btn-sm btn-success" onclick="downloadCSV()">Download CSV</button>
                <button class="btn btn-sm btn-warning" onclick="printPage()">Print Page</button>
        <a href="/admin/completed_loan_enquiry"><button class="btn btn-sm btn-danger">Completed Enquiries</button></a>

            </div>
            <div class="mb-3 col-md-12 text-center">
                <h3>Loan Enquiry list</h3>
            </div>
            <div class="mb-3 col-md-12">
            <div class="table-wrapper" style="overflow-x: auto; -webkit-overflow-scrolling: touch;">

                <table  class="table table-sm table-bordered">
                    <tr>
                        <th>Action</th>
                        <th>Sr.no</th>
                        <th>Enquiry For</th>
                        <th>Enquirer Name</th>
                        <th>Enquirer Email</th>
                        <th>Enquirer Mobile No</th>
                        <th>Enquirer City</th>
                        <th>Enquirer Message</th>

                    </tr>
                    <% for(var i=0; i<enq_details.length; i++) {%>
                        <tr>
                            <td>
                                <a href="complete_loan_enquiry/<%-enq_details[i].loan_enquiry_id%>">
                                    <button class="btn btn-primary">Complete</button>
                                </a>
                            </td>
                            <!-- <td>
                                <a href="/admin/delete_enquiry_details/<%=enq_details[i].loan_enquiry_id%>" title="Delete Enquiry Type">
                                    <button class="btn btn-sm btn-danger" onclick="alert('Are You Sure...?')">
                                <i class="fa fa-trash"></i>
                            </button>
                                </a>
                            </td> -->
                            <td>
                                <%= 1+i %>
                            </td>
                            <td>
                                <%= enq_details[i].loan_name %>
                            </td>
                            <td>
                                <%= enq_details[i].enquirers_name %>
                            </td>
                            <td>
                                <%= enq_details[i].enquirers_email %>
                            </td>
                            <td>
                                <%= enq_details[i].enquirers_mobile %>
                            </td>
                            <td>
                                <%= enq_details[i].enquirers_city %>
                            </td>
                            <td>
                                <%= enq_details[i].enquirers_message %>
                            </td>
                        </tr>
                        <% } %>
                </table>

                


            </div>

            <div class="col-md-12 mt-5">
                <nav aria-label="Page navigation example">
                    <ul class="pagination justify-content-center">
                       
                        <% for(var i=1; i<=total_pages; i++) { %>
    
                            <li class="page-item">
                                <a class="page-link" href="/admin/loan_enquiry?page_no=<%=i%>">
                                    <%= i %>
                                </a>
                            </li>
                            <% } %>
    
                                
                    </ul>
                </nav>
            </div>
            
            </div>

        </div>
    </div>



    <%- include('footer.ejs') %>